<% include ../partials/_header %>

  <body>
    <div class="container">
      <h1>Create League</h1>
    </div>

    <div class="container">
      <form>
        <div class="form-group">
          <label for="email">League Name:</label>
          <input type="text" class="form-control" id="name">
        </div>
        <div class="form-group">
          <label for="email">Number of Teams:
          <select class="form-control" id="size">
                <option>4</option>
                <option>5</option>
                <option>6</option>
                <option>7</option>
                <option>8</option>
                <option>9</option>
                <option>10</option>
                <option>11</option>
                <option selected="selected">12</option>
                <option>13</option>
                <option>14</option>
                <option>15</option>
                <option>16</option>
                <option>17</option>
                <option>18</option>
                <option>19</option>
                <option>20</option>
                <option>21</option>
                <option>22</option>
                <option>23</option>
                <option>24</option>
                <option>25</option>
                <option>26</option>
                <option>27</option>
                <option>28</option>
                <option>29</option>
                <option>30</option>
                <option>31</option>
                <option>32</option>
              </select>
            </label>
        </div>
        <div class="form-group">
          <label for="scoring">Scoring Type:</label>
          <div class="radio">
            <label class="radio-inline"><input type="radio" name="standard">Standard</label>
            <label class="radio-inline"><input type="radio" name="0.5">0.5 PPR</label>
            <label class="radio-inline"><input type="radio" name="ppr">PPR</label>
          </div>
        </div>
        <div class="form-group">
          <label for="league">League Type:</label>
          <div class="radio">
            <label class="radio-inline"><input type="radio" name="redraft">Redraft</label>
            <label class="radio-inline"><input type="radio" name="dynasty">Dynasty</label>
            <label class="radio-inline"><input type="radio" name="keeper">Keeper</label>
          </div>
        </div>
        <div class="form-group">
          <label for="email">Number of Keepers:
          <select class="form-control" id="size">
                <option>0</option>
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
                <option>6</option>
                <option>7</option>
                <option>8</option>
