                <option>17</option>
                <option>18</option>
                <option>19</option>
                <option>20</option>
              </select>
            </label>
        </div>
        <div class="form-group">
          <label for="position">Roster Positions:</label>
          <div class="container">
            <div class="row">
              <div class="col-xs-1">
                <label class="text-inline">QB:
                  <select class="form-control" id="qb">
                    <option>0</option>
                    <option selected="selected">1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                  </select>
                </label>
              </div>
              <div class="col-xs-1">
                <label class="text-inline">RB:
                  <select class="form-control" id="rb">
                    <option>0</option>
                    <option>1</option>
                    <option selected="selected">2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                  </select>
                </label>
              </div>
              <div class="col-xs-1">
                <label class="text-inline">WR:
                  <select class="form-control" id="wr">
                    <option>0</option>
                    <option>1</option>
                    <option selected="selected">2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                  </select>
                </label>
              </div>
              <div class="col-xs-1">
                <label class="text-inline">TE:
                  <select class="form-control" id="te">
                    <option>0</option>
                    <option selected="selected">1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                  </select>
                </label>
              </div>
            </div>

            <div class="row">
              <div class="col-xs-1">
                <label class="text-inline">RB/WR/TE:
                  <select class="form-control" id="rb/wr/te">
                    <option>0</option>
                    <option>1</option>
                    <option selected="selected">2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                  </select>
                </label>
              </div>
              <div class="col-xs-1">
