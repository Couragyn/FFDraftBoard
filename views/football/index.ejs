                    <option>1</option>
                    <option selected="selected">2</option>
                    <option>3</option>
                    <option>4</option>
                  </select>
                </label>
              </div>
              <div class="col-xs-1">
                <label class="text-inline">Bench:
                  <select class="form-control" id="bench">
                    <option>0</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
